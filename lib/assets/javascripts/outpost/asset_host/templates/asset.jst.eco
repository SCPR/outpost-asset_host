<div class="thumbnail <%= @asset.use %>" data-asset-id="<%= @asset.id %>">
  <img src="<%= @asset.urls.lsquare %>" width="<%= @asset.sizes.lsquare.width %>" height="<%= @asset.sizes.lsquare.height %>" draggable="false" />

  <div class="asset-info">
    <h6><%= @asset.title %></h6>
    <small class="muted"><%= @asset.caption %></small>
  </div>
</div>
